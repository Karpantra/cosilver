<div class="form-wrapping col-xs-12">
  <br>
  <br>
  <div class="col-xs-10 col-offset-1 col-sm-6">
  <%= simple_form_for @flat do |f| %>
    <%= f.input :category, placeholder: "Category", collection: ["Appartment", "House"] %>
    <%= f.input :address, placeholder: "Address" %>
    <%= f.input :city, placeholder: "City" %>
    <%= f.input :service_charges, placeholder: "Service charge"%>
    <%= f.input :deposit, placeholder: "Deposit" %>

  </div>
  <div class="col-xs-10 col-offset-1 col-sm-6">
    <%= f.input :flat_area, placeholder: "Flat area" %>
    <%= f.input :max_roommmates, placeholder: "Max roommates" %>
    <%= f.input :number_pieces, placeholder: "Number of pieces" %>
    <%= f.input :smoker %>
    <%= f.input :animals %>
  </div>
    <%= f.submit "Add a Wild Place", class: "btn btn-success" %>
  <% end %>
</div>
<%= content_for(:after_js) do %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;key=AIzaSyDANjx3bosEtIyzJaoWs50Wnt6nt_1rmxU"></script>
<% end %>
